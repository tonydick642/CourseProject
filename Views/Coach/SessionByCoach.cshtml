@model IEnumerable<CourseProject.Models.Session>
<h2>Post Progress Report</h2>
@{ 
    if (Model.Count() < 1)
    {
        <p>You don't have any sessions yet. <a asp-for="AddSession"> Add a session</a></p>
    }
    else
    {
        <h3>All Sessions</h3>
        <table border="1">
            <tr>
                <th>Session ID</th>
                <th>Seats Available</th>
                <th>Lesson ID</th>
                <th>Progress Report</th>
            </tr>
            @foreach (var session in Model)
            {
        <tr>
            <td>@session.SessionId</td>
            <td>@session.SeatsAvailable</td>
            <td>@session.LessonId</td>
            <td>
                <a asp-controller="Coach"
                   asp-action="PostProgressReport"
                   asp-route-id="@session.SessionId">Post Progress Report</a>
            </td>
        </tr>
            }
        </table>
    }
}
<p><a asp-action="Index">Back to coach home</a></p>