@model IEnumerable<CourseProject.ViewModels.SwimmerEnrollSessionViewModel>
    {
        <p>No session in database yet.</p>
    }
    else
    {
        <h3>All Sessions</h3>
        <table border="1">
            <tr>
                <th>Coach</th>
                <th>Lesson</th>
                <th>Tuition</th>
                <th>Session</th>
                <th>Seats Available</th>
                <th>Enroll in session</th>
            </tr>
            @foreach (var session in Model)
            {
        <tr>
            <td>@session.Coach</td>
            <td>@session.Lesson</td>
            <td>@session.Tuition</td>
            <td>@session.session</td>
            <td>@session.SeatsAvailable</td>
        </tr>
            }
        </table>
        }
    }


