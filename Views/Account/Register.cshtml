@model CourseProject.Models.Admin

<h1>User Registration</h1>
<form asp-controller="Account" asp-action="Login" method="post">
    <label asp-for="AdminEmail">Email Address</label>
    <input asp-for="AdminEmail" />
    <br />
    <label asp-for="AdminPassword">Password</label>
    <input asp-for="AdminPassword" />
    <br />
    <label asp-for="AdminPassword">Confirm Password</label>
    <input asp-for="AdminPassword" />
    <br />
    <button type="submit">Register</button>
</form>
<h5>
    <a asp-action="Login">Back to Login</a>
</h5>






